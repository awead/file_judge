<%= content_tag :h1, "Case ID "+@case.id.to_s %>

<%= content_tag :dl, class: "dl-horizontal" do %>
  <%= content_tag :dt, "Name" %>
  <%= content_tag :dd, @case.name %>
  <%= content_tag :dt, "Path" %>
  <%= content_tag :dd, @case.path %>
  <%= content_tag :dt, "Verdict" %>
  <%= content_tag :dd, @case.verdict %>
<% end %>

<%= render partial: "tickets" unless @tickets.empty? %>